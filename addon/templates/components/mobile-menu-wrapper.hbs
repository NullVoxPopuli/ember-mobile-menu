{{yield
  (hash
    mobile-menu=(component "mobile-menu"
      embed=embed
      onOpen=(action "didOpenMenu")
      onClose=(action "didCloseMenu")
    )
    toggle=(component "mobile-menu-toggle"
      onClick=(action "toggle")
    )
    actions=(hash
      toggle=(action "toggle")
      close=(action "close")
    )
  )
}}